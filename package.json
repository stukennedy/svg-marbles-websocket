{
  "name": "ws-marbles",
  "version": "1.0.1",
  "description": "Realtime WebSocket visualization for RxJS Observables",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "default": "./dist/client.global.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rxjs",
    "marble",
    "diagram",
    "websocket",
    "realtime",
    "visualization",
    "observable"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0"
  }
}
